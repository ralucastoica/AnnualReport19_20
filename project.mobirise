          "_name": "testimonials",
          "_customHTML": "<section class=\"carousel1 slider testimonials-carousel\" group=\"Carousels &amp; Sliders\" plugins=\"amp-main, amp-carousel\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Title</header>\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked=\"\">\n        <header>Title</header>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\">\n        <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Signature\" name=\"showSignature\">\n        <header>Cards</header>\n        <input type=\"color\" title=\"Arrow Color\" name=\"arrowColor\" value=\"#222222\">\n        <input type=\"color\" title=\"Cards Bg\" name=\"cardsBg\" value=\"#ffffff\">    \n        <input type=\"checkbox\" title=\"ShowCardTitle\" name=\"ShowCardTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"ShowCardsSignature\" name=\"ShowCardsSignature\" checked=\"\">\n        <input type=\"checkbox\" title=\"ShowCardImg\" name=\"ShowCardImg\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Cards Height\" name=\"carouselHeight\" min=\"200\" max=\"1000\" step=\"10\" value=\"350\">\n        <select title=\"Reviews\" name=\"imagesAmount\">\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\" selected=\"\">5</option>\n            <option value=\"6\">6</option>\n        </select>\n        \n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/bg5-2000x1502.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#e49f37\" selected=\"\">\n        </fieldset>\n        \n        <input type=\"checkbox\" title=\"2 Background Color\" name=\"twoBgColor\" condition=\"bg.type == 'color'\" checked=\"\"> \n<input type=\"range\" inline=\"\" title=\"Height 2 Bg\" name=\"overlayHeight\" min=\"20\" max=\"80\" step=\"10\" value=\"80\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">  \n<input type=\"color\" title=\"2 Color\" name=\"twoColor\" value=\"#edd873\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">\n        \n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked=\"\" condition=\"bg.type!='color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#000000\" condition=\"bg.type!='color' &amp;&amp; overlay\">\n        <input type=\"range\" inline=\"\" title=\"Overlay Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"bg.type!='color' &amp;&amp; overlay\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n    \n    <div class=\"mbr-overlay\" mbr-if=\"(overlay &amp;&amp; bg.type !== 'color') || (twoBgColor &amp;&amp; bg.type == 'color')\"></div>\n    <!-- <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div> -->\n    <div class=\"\" mbr-class=\"{'container':!fullWidth,\n                     'container-fluid':fullWidth}\">\n        <div class=\"text__block mbr-col-sm-12 mbr-col-md-8 mbr-m-auto\" mbr-if=\"showTitle || showSubtitle\">\n            <h2 class=\"mbr-section-subtitle mbr-pb-2 mbr-fonts-style align-center mbr-regular\" mbr-theme-style=\"display-6\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-subtitle\">MORE THAN JUST BUSINESS \n            </h2>\n            <h2 class=\"mbr-fonts-style mbr-section-title align-center\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-title\"><b>Scroll left or right to meet hte Team&nbsp;</b></h2>\n            \n            <p class=\"mbr-fonts-style mbr-text align-center testimonials__subtitle\" mbr-if=\"showSignature\" mbr-theme-style=\"display-7\" data-app-selector=\".testimonials__subtitle\">Phosfluorescently engage worldwide methodologies with web-enabled technology. Interactively coordinate proactive e-commerce via process-centric outside the box thinking.</p>\n        </div>\n        <div class=\"slider-box\"><amp-carousel class=\"carousel\" height=\"{{carouselHeight}}\" layout=\"fixed-height\" type=\"carousel\"><div class=\"text-wrap\">\n                        <div class=\"text-content\">\n\n                            <div class=\"testimonial-info mbr-flex\" mbr-if=\"ShowCardTitle || ShowCardsSignature || ShowCardImg\">\n                            <div class=\"testimonial-info-title align-left\"><h3 class=\"mbr-fonts-style mbr-section-title card__title\" mbr-theme-style=\"display-6\" mbr-if=\"ShowCardTitle\" data-app-selector=\".card__title\"><b>Message from the Director</b></h3>  <p class=\"mbr-fonts-style mbr-text card-signature mbr-bold\" mbr-theme-style=\"display-7\" mbr-if=\"ShowCardsSignature\" data-app-selector=\".card-signature\">Nicole Bois</p>\n                                 \n                            </div>\n                            <div class=\"card-img\" mbr-if=\"ShowCardImg\">\n                        <amp-img src=\"@PROJECT_PATH@/assets/images/cmcteam-1.jpeg\" alt=\"image\" height=\"350\" width=\"341.5356711003628\" layout=\"responsive\" class=\"mobirise-loader\">\n                        <div placeholder=\"\" class=\"placeholder\"></div>\n                        <img src=\"@PROJECT_PATH@/assets/images/cmcteam-1.jpeg\" class=\"mbr-temp\" alt=\"\" title=\"\">\n                    </amp-img>\n                    </div>\n                      </div>     \n                            <p class=\"mbr-fonts-style mbr-text card-text align-left\" mbr-t